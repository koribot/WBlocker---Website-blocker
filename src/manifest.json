{
  "manifest_version": 3,
  "name": "WBlocker Website Blocker -  Block any website using chrome extension",
  "version": "0.0.1",
  "description": "Block any website using chrome extension",
  "permissions": [
      "activeTab",
      "tabs",
      "storage",
      "scripting"
  ],
  "commands":{
    "go-to-amazon":{
      "suggested_key":{
        "default": "Alt+1"
      },
      "description": "WBlocker - Block any website using chrome extension"
    }
  },
  "action": {
      "default_popup": "html/popup.html"
  },
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["dist/utils/socket.script.js", "dist/utils/modal.ts.js"] 
    },
    {
      "matches": ["https://www.google.com/*"],
      "js": ["dist/scripts/content.script.ts.js"]
    }
  ],
  "background": {
      "service_worker": "dist/scripts/background.script.ts.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
      {
          "resources": [
            "dist/utils/*"
          ],
          "matches": [
              "<all_urls>"
          ]
      }
  ],
  "icons": {
      "16": "icons/logo.png",
      "48": "icons/16x16.png",
      "128": "icons/16x16.png"
  },
  "incognito": "spanning"
}